{
	"name": "application",
	"description": "my application.",
	"keywords": [],
	"license": "MIT",
	"type": "project",
	"require": {

	},
	"require-dev": {

	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
